{
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/uuid": "^8.3.4",
    "babel-jest": "^29.0.2",
    "cspell": "^6.8.1",
    "eslint": "^8.23.0",
    "eslint-config-canonical": "^37.0.3",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "name": "battler",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:cspell",
    "lint:cspell": "cspell './**/*.ts' --no-progress --gitignore",
    "lint:eslint": "eslint --fix './**/*.ts' --cache",
    "lint:prettier": "prettier --ignore-path .gitignore --check './**/*.ts'",
    "prepare": "husky install",
    "test": "NODE_ENV=test jest --watchAll --verbose"
  }
